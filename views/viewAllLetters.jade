extend layout

block content
    ul.nav.nav-tabs
        li.active
            a(href="#toMe", data-toggle="tab") To Me
        li
            a(href="#forMe", data-toggle="tab") For Me
    p
    div.tab-content(id='myTabContent')
        div.tab-pane.fade.active.in(id='toMe')
            table.table.table-striped.table-hover
                tbody
                    -for (var a = 0; a < 10; a++)
                        tr
                            td Letter to me #{a}
        div.tab-pane.fade(id='forMe')
            table.table.table-striped.table-hover
                tbody
                    -for (var a = 0; a < 10; a++)
                        tr
                            td Letter for me #{a}